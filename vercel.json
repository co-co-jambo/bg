{
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": "dist/public",
  "installCommand": "yarn install --ignore-engines",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api/server"
    },
    {
      "source": "/api/oauth/:path*",
      "destination": "/api/server"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ]
}
